{
  "manifest_version": 2,

  "name": "Textifier",
  "description": "This extension disables loading of all the images on the current webpage",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
  
  "content_scripts": [
    
        "matches": "http:http://photobucket.com/",
        "js": "/content_script.js"
    ]
}
